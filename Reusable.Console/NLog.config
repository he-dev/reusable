            [Timestamp],
            [Environment],
            [Product],
            [Logger],
            [Transaction],
            [Expected],
            [Actual],
            [AreEqual],
            [Event],
            [Elapsed],
            [Result],
            [Message],
            [Exception]
				) VALUES (
            @Layer,
            @Level,
            @Timestamp,
            @Environment,
            @Product,
            @Logger,
            NULLIF(@Transaction, ''),
            NULLIF(@Expected, ''),
            NULLIF(@Actual, ''),
            NULLIF(@AreEqual, ''),
            NULLIF(@Event, ''),
            NULLIF(@Elapsed, ''),
            NULLIF(@Result, ''),
            NULLIF(@Message, ''),
            NULLIF(@Exception, '')
				)
